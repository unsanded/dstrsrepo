pkgname=qutim-utility-qip-smiles
pkgver=0.1
pkgrel=1
pkgdesc="Utility to convert smiles from qip to qutim"
arch=('i686' 'x86_64')
url="http://qutim.org"
license=('GPL')
makedepends=()
depends=(qt)
conflicts=()
provides=()

source=(http://qutim.noload.ru/qIMhm/smiles.tar.bz2)                              
md5sums=()

build() {
 . ../../optimizations-$CARCH

 cd $srcdir/smiles

 qmake
 make || return 1

 mkdir -p $pkgdir/usr/bin
 install -Dm 755 $srcdir/$_svnmod-build/lib$_svnmod.so $pkgdir/usr/bin
 cd ..
 rm -rf smiles
}
